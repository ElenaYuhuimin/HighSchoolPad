package com.emodou.domain;


import java.util.ArrayList;
import java.util.List;

/**
 * Created by woody on 2015/8/25.
 */
public class EmodouWordUnit {
    private int id;
    private String bookid;
    private String unitid;
    private String unitname;
    private List<EmodouWordClass> wordclass;

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getBookid() {
        return bookid;
    }

    public void setBookid(String bookid) {
        this.bookid = bookid;
    }

    public String getUnitid() {
        return unitid;
    }

    public void setUnitid(String unitid) {
        this.unitid = unitid;
    }

    public String getUnitname() {
        return unitname;
    }

    public void setUnitname(String unitname) {
        this.unitname = unitname;
    }

    public List<EmodouWordClass> getWordclass() {
        return wordclass;
    }

    public void setWordclass(List<EmodouWordClass> wordclass) {
        this.wordclass = wordclass;
    }

    @Override
    public String toString() {
        return "WordUnit{" +
                "id=" + id +
                ", bookid='" + bookid + '\'' +
                ", unitid='" + unitid + '\'' +
                ", unitname='" + unitname + '\'' +
                ", word=" + wordclass +
                '}';
    }
}
